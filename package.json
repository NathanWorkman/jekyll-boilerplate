{
  "name": "jekyll-boilerplate",
  "version": "1.0.0",
  "description": "Jekyll Boilerplate is my personal starter for kicking out static sites.",
  "main": "_assets/index.js",
  "scripts": {
    "clean": "bundle exec jekyll clean",
    "dev": "run-p dev:**",
    "dev:webpack": "webpack-dev-server --config webpack.dev.js --hot",
    "dev:jekyll": "bundle exec jekyll serve --livereload --incremental --profile",
    "build": "npm run build:webpack && npm run build:jekyll",
    "build:preview": "npm run build:webpack && npm run build:jekyll:preview",
    "build:jekyll": "bundle exec jekyll build",
    "build:jekyll:preview": "npm run build:jekyll",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js --scripts-prepend-node-path"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NathanWorkman/jekyll-boilerplate.git"
  },
  "author": "Nathan Workman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NathanWorkman/jekyll-boilerplate/issues"
  },
  "homepage": "https://github.com/NathanWorkman/jekyll-boilerplate#readme",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "bootstrap.native": "^3.0.9",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "assets-webpack-plugin": "^5.0.2",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "exports-loader": "^1.1.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^6.0.0",
    "image-webpack-loader": "^6.0.0",
    "imports-loader": "^1.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "plugin-error": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^9.0.1",
    "style-loader": "^1.1.4",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.1.4"
  }
}
